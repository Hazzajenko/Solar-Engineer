{
  "name": "solarengineer",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "tauri": "tauri",
    "start-desktop": "tauri dev",
    "start": "nx serve design-app",
    "start-host-test": "nx serve design-app --host=0.0.0.0 --port=4200",
    "start-host": "nx serve design-app --host=0.0.0.0 --port=4200 --disable-host-check",
    "start-host-2": "nx serve design-app --host=0.0.0.0 --port=4200 --public-host dev.solarengineer.app",
    "start-host-dev": "nx serve design-app --host dev.solarengineer.app --port=4200 --disable-host-check",
    "start-web-ssl": "nx serve web --ssl",
    "start-no-ssl": "nx serve web --ssl false",
    "start-nginx": "nx serve web --ssl false --host solarengineer.local",
    "start-solarengineer.net": "nx serve web --ssl false --host solarengineer.net",
    "start-design-solarengineer.net": "nx serve design-app --ssl false --host solarengineer.net",
    "start-svelte": "nx serve svelte-design-app --ssl false --host solarengineer.net",
    "start-solid": "nx serve solid-design-app --ssl --host solarengineer.net",
    "start-ssl": "nx serve --ssl",
    "start-ssl-certs": "nx serve web --ssl --ssl-key /ssl/localhost.key --ssl-cert /ssl/localhost.crt",
    "proxy": "nx serve --proxy-config src/proxy.conf.json",
    "inspect": "nx serve --inspect",
    "build": "nx build design-app --configuration=production --verbose",
    "watch": "nx build --watch --configuration development",
    "test": "xo && nx test",
    "postinstall": "node ./decorate-angular-cli.js",
    "dead-code": "ts-prune -p tsconfig.base.json -s '*.test.ts?' | grep -v cssExports | grep -v 'used in module'",
    "generate-icons": "svg-to-ts"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./libs/shared/svgs/src/lib/**/*.svg"
    ],
    "outputDirectory": "./libs/shared/svgs/src/dist",
    "interfaceName": "AppIcon",
    "typeName": "appIcon",
    "prefix": "appIcon",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs"
      ]
    },
    "fileName": "app-icon.types",
    "additionalModelOutputPath": "./libs/shared/svgs/src/lib/dist/types",
    "compileSources": true
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^16.0.0",
