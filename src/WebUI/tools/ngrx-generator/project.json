{
	"name": "ngrx-generator",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "tools/ngrx-generator/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": [
				"{options.outputPath}"
			],
			"options": {
				"outputPath": "dist/tools/ngrx-generator",
				"main": "tools/ngrx-generator/src/index.ts",
				"tsConfig": "tools/ngrx-generator/tsconfig.lib.json",
				"assets": [
					"tools/ngrx-generator/*.md",
					{
						"input": "./tools/ngrx-generator/src",
						"glob": "**/!(*.ts)",
						"output": "./src"
					},
					{
						"input": "./tools/ngrx-generator/src",
						"glob": "**/*.d.ts",
						"output": "./src"
					},
					{
						"input": "./tools/ngrx-generator",
						"glob": "generators.json",
						"output": "."
					},
					{
						"input": "./tools/ngrx-generator",
						"glob": "executors.json",
						"output": "."
					}
				]
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"tools/ngrx-generator/**/*.ts",
					"tools/ngrx-generator/package.json",
					"tools/ngrx-generator/generators.json"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": [
				"{workspaceRoot}/coverage/{projectRoot}"
			],
			"options": {
				"jestConfig": "tools/ngrx-generator/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": []
}
