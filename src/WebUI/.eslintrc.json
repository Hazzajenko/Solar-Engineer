{
  "root": true,
  "ignorePatterns": [
    "**/*"
  ],
  "plugins": [
    "@nrwl/nx"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx",
        "*.svelte"
      ],
      "rules": {
        "@angular-eslint/component-selector": "off",
        "@nrwl/nx/enforce-module-boundaries": [
          "error",
          {
            "enforceBuildableLibDependency": true,
            "allow": [],
            "depConstraints": [
              {
                "sourceTag": "auth:api",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared"
                ]
              },
              {
                "sourceTag": "auth:effects",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared",
                  "auth:store",
                  "auth:facades",
                  "auth:api",
                  "projects:store",
                  "projects:shared",
                  "projects:facades"
                ]
              },
              {
                "sourceTag": "auth:facades",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared",
                  "auth:store",
                  "auth:api"
                ]
              },
              {
                "sourceTag": "auth:store",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared",
                  "auth:store"
                ]
              },
              {
                "sourceTag": "auth:guards",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared",
                  "auth:facades"
                ]
              },
              {
                "sourceTag": "auth:interceptors",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared",
                  "auth:facades"
                ]
              },
              {
                "sourceTag": "auth:shared",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "auth:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:actions",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:shared",
                  "project-id:shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:repositories",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:shared",
                  "grid-layout:actions",
                  "grid-layout:utils",
                  "project-id:facades",
                  "project-id:shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:services",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:shared",
                  "grid-layout:actions",
                  "grid-layout:utils",
                  "project-id:facades",
                  "project-id:shared",
                  "projects:facades",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:utils",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:shared",
                  "grid-layout:actions",
                  "grid-layout:utils",
                  "project-id:facades",
                  "project-id:store",
                  "project-id:shared",
                  "projects:facades",
                  "projects:store",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:feature",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:feature",
                  "grid-layout:shared",
                  "grid-layout:shared-ui",
                  "grid-layout:actions",
                  "grid-layout:utils",
                  "grid-layout:services",
                  "grid-layout:repositories",
                  "project-id:facades",
                  "project-id:utils",
                  "project-id:shared",
                  "projects:facades",
                  "projects:utils",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "grid-layout:shared",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "grid-layout:shared",
                  "project-id:shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "project-id:api",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "project-id:effects",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:shared",
                  "project-id:api",
                  "project-id:store",
                  "project-id:facades",
                  "project-id:utils",
                  "projects:shared",
                  "projects:facades",
                  "projects:store"
                ]
              },
              {
                "sourceTag": "project-id:facades",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:shared",
                  "project-id:store",
                  "projects:shared",
                  "projects:store"
                ]
              },
              {
                "sourceTag": "project-id:resolvers",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:shared",
                  "project-id:facades",
                  "projects:shared",
                  "projects:facades"
                ]
              },
              {
                "sourceTag": "project-id:store",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "project-id:feature",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "project-id:feature",
                  "project-id:shared",
                  "projects:shared",
                  "projects:facades",
                  "project-id:facades",
                  "grid-layout:feature"
                ]
              },
              {
                "sourceTag": "projects:api",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "projects:shared"
                ]
              },
              {
                "sourceTag": "projects:effects",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "projects:shared",
                  "projects:facades",
                  "projects:api",
                  "projects:store"
                ]
              },
              {
                "sourceTag": "projects:facades",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "projects:shared",
                  "projects:store"
                ]
              },
              {
                "sourceTag": "projects:store",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "projects:shared",
                  "projects:store"
                ]
              },
              {
                "sourceTag": "projects:feature",
                "onlyDependOnLibsWithTags": [
                  "shared",
                  "projects:shared",
                  "projects:facades"
                ]
              },
              {
                "sourceTag": "shared",
                "onlyDependOnLibsWithTags": [
                  "shared"
                ]
              },
              {
                "sourceTag": "*",
                "onlyDependOnLibsWithTags": [
                  "*"
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "extends": [
        "plugin:@nrwl/nx/typescript"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "extends": [
        "plugin:@nrwl/nx/javascript"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.svelte"
      ]
    },
    {
      "files": [
        "*.spec.ts",
        "*.spec.tsx",
        "*.spec.js",
        "*.spec.jsx"
      ],
      "env": {
        "jest": true
      },
      "rules": {}
    },
    {
      "files": "*.json",
      "parser": "jsonc-eslint-parser",
      "rules": {}
    }
  ]
}
