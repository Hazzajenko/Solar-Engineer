{
  "name": "design-app-feature-design-layout",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/design-app/feature-design-layout/src",
  "prefix": "app",
  "tags": ["design-app:feature"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/design-app/feature-design-layout/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/design-app/feature-design-layout/**/*.ts",
          "libs/design-app/feature-design-layout/**/*.html"
        ]
      }
    }
  }
}
