{
  "name": "deprecated-design-app-canvas",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/deprecated/design-app/canvas/src",
  "prefix": "app",
  "tags": [
    "canvas:feature"
  ],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/deprecated/design-app/canvas/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/deprecated/design-app/canvas/**/*.ts",
          "libs/deprecated/design-app/canvas/**/*.html"
        ]
      }
    }
  }
}
