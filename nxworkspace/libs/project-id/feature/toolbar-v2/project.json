{
  "name": "project-id-feature-toolbar-v2",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/project-id/feature/toolbar-v2/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/project-id/feature/toolbar-v2/ng-package.json",
        "tailwindConfig": "libs/project-id/feature/toolbar-v2/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/project-id/feature/toolbar-v2/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/project-id/feature/toolbar-v2/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/project-id/feature/toolbar-v2/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/project-id/feature/toolbar-v2/**/*.ts",
          "libs/project-id/feature/toolbar-v2/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
