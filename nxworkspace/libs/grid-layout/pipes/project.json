    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/grid-layout/pipes/**/*.ts", "libs/grid-layout/pipes/**/*.html"]
      }
    }
  },
  "tags": []
}
