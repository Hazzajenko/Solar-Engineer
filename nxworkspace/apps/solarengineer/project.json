{
  "name": "solarengineer",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/solarengineer/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/solarengineer",
        "index": "apps/solarengineer/src/index.html",
        "main": "apps/solarengineer/src/main.ts",
        "polyfills": "apps/solarengineer/src/polyfills.ts",
        "tsConfig": "apps/solarengineer/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/solarengineer/src/favicon.ico", "apps/solarengineer/src/assets"],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/solarengineer/src/styles.scss"
        ],
        "scripts": [],
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/solarengineer/src/environments/environment.ts",
              "with": "apps/solarengineer/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "solarengineer:build",
        "proxyConfig": "apps/solarengineer/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "browserTarget": "solarengineer:build:production"
        },
        "development": {
          "browserTarget": "solarengineer:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "solarengineer:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/solarengineer/src/test.ts",
        "polyfills": "apps/solarengineer/src/polyfills.ts",
        "tsConfig": "apps/solarengineer/tsconfig.spec.json",
        "karmaConfig": "apps/solarengineer/karma.conf.js",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/solarengineer/src/favicon.ico", "apps/solarengineer/src/assets"],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/solarengineer/src/styles.scss"
        ],
        "scripts": [],
        "webWorkerTsConfig": "apps/solarengineer/tsconfig.worker.json"
      }
    }
  }
}
