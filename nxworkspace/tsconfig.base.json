      "@auth/guards": ["libs/auth/guards/src/index.ts"],
      "@auth/interceptors": ["libs/auth/interceptors/src/index.ts"],
      "@auth/shared/models": ["libs/auth/shared/models/src/index.ts"],
      "@grid-layout/data-access/actions": ["libs/grid-layout/data-access/actions/src/index.ts"],
      "@grid-layout/data-access/services": ["libs/grid-layout/data-access/services/src/index.ts"],
      "@grid-layout/data-access/utils": ["libs/grid-layout/data-access/utils/src/index.ts"],
      "@grid-layout/feature/blocks/block-disconnection-point": [
        "libs/grid-layout/feature/blocks/block-disconnection-point/src/index.ts"
      ],
      "@grid-layout/feature/blocks/block-panel": [
        "libs/grid-layout/feature/blocks/block-panel/src/index.ts"
      ],
      "@grid-layout/feature/blocks/block-tray": [
        "libs/grid-layout/feature/blocks/block-tray/src/index.ts"
      ],
      "@grid-layout/feature/blocks/shared-ui": [
        "libs/grid-layout/feature/blocks/shared-ui/src/index.ts"
      ],
      "@grid-layout/feature/index": ["libs/grid-layout/feature/index/src/index.ts"],
      "@grid-layout/feature/keymap": ["libs/grid-layout/feature/keymap/src/index.ts"],
      "@grid-layout/feature/string-stats": ["libs/grid-layout/feature/string-stats/src/index.ts"],
      "@grid-layout/shared/models": ["libs/grid-layout/shared/models/src/index.ts"],
      "@grid-layout/utils/factory": ["libs/grid-layout/utils/factory/src/index.ts"],
      "@home/ui": ["libs/home/ui/src/index.ts"],
