      "@project-id/data-access/effects": ["libs/project-id/data-access/effects/src/index.ts"],
      "@project-id/data-access/facades": ["libs/project-id/data-access/facades/src/index.ts"],
      "@project-id/data-access/injections": ["libs/project-id/data-access/injections/src/index.ts"],
      "@project-id/data-access/resolvers": ["libs/project-id/data-access/resolvers/src/index.ts"],
      "@project-id/data-access/services": ["libs/project-id/data-access/services/src/index.ts"],
      "@project-id/data-access/store": ["libs/project-id/data-access/store/src/index.ts"],
      "@project-id/feature/local": ["libs/project-id/feature/local/src/index.ts"],
      "@project-id/feature/toolbar": ["libs/project-id/feature/toolbar/src/index.ts"],
      "@project-id/feature/web": ["libs/project-id/feature/web/src/index.ts"],
      "@project-id/shared/models": ["libs/project-id/shared/models/src/index.ts"],
