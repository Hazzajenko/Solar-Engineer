# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

# max_line_length = off


[*.ts]
quote_type = single
# max_line_length = off

[*.md]
max_line_length = off
trim_trailing_whitespace = false
