    "mobile": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/mobile",
      "sourceRoot": "apps/mobile/src",
      "prefix": "app",
      "architect": {
        "build": {
